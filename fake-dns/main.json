{
    "log": {
        "level": "warn"
    },
    "experimental": {
        "cache_file": {
            "enabled": true,
            "store_fakeip": true
        },
        "clash_api": {
            "default_mode": "rule",
            "external_controller": "127.0.0.1:9090",
            "external_ui": "fx505",
            "external_ui_download_detour": "direct",
            "external_ui_download_url": "https://github.com/MetaCubeX/Yacd-meta/archive/gh-pages.zip"
        }
    },
    "dns": {
        "fakeip": {
            "enabled": true,
            "inet4_range": "198.18.0.0/15",
            "inet6_range": "fc00::/18"
        },
        "independent_cache": true,
        "rules": [
            {
                "query_type": [
                    "HTTPS",
                    "SVCB"
                ],
                "server": "dns-block"
            },
            {
                "query_type": [
                    32,
                    33
                ],
                "server": "dns-block"
            },
            {
                "domain_suffix": ".lan",
                "server": "dns-block"
            },
            {
                "domain_suffix": [
                    ".arpa",
                    ".arpa."
                ],
                "server": "dns-block"
            },
            {
                "outbound": "direct",
                "server": "dns-direct",
                "rewrite_ttl": 20
            },
            {
                "mode": "or",
                "rules": [
                    {
                        "domain_suffix": [
                            ".ir"
                        ],
                        "rule_set": [
                            "geoip-ir",
                            "geosite-ir"
                        ]
                    }
                ],
                "server": "dns-direct",
                "type": "logical"
            },
            {
                "disable_cache": true,
                "inbound": [
                    "tun-in"
                ],
                "server": "dns-fake"
            }
        ],
        "servers": [
            {
                "address": "https://8.8.8.8/dns-query",
                "address_resolver": "dns-local",
                "strategy": "prefer_ipv4",
                "tag": "dns-remote"
            },
            {
                "address": "1.1.1.1",
                "address_resolver": "dns-local",
                "detour": "direct",
                "strategy": "ipv4_only",
                "tag": "dns-direct"
            },
            {
                "address": "local",
                "detour": "bypass",
                "tag": "dns-local"
            },
            {
                "address": "rcode://success",
                "tag": "dns-block"
            },
            {
                "address": "fakeip",
                "tag": "dns-fake",
                "strategy": "ipv4_only"
            }
        ]
    },
    "inbounds": [
        {
            "listen": "127.0.0.1",
            "listen_port": 6450,
            "override_address": "8.8.8.8",
            "override_port": 53,
            "tag": "dns-in",
            "type": "direct"
        },
        {
            "auto_route": true,
            "strict_route": true,
            "exclude_package": [
                "com.android.captiveportallogin",
                "com.biglybt.android.client",
                "org.openobservatory.ooniprobe",
                "org.proninyaroslav.libretorrent"
            ],
            "inet4_address": "172.19.0.1/30",
            "inet6_address": "fdfe:dcba:9876::1/126",
            "platform": {
                "http_proxy": {
                    "enabled": true,
                    "server": "127.0.0.1",
                    "server_port": 2080
                }
            },
            "sniff": true,
            "stack": "gvisor",
            "mtu": 1500,
            "tag": "tun-in",
            "type": "tun"
        }
    ],
    "outbounds": [
        {
            "tag": "proxy",
            "interrupt_exist_connections": true,
            "type": "selector",
            "default": "ðŸš€ Fastest",
            "outbounds": null
        },
        {
            "tag": "ðŸš€ Fastest",
            "interrupt_exist_connections": true,
            "interval": "20m",
            "idle_timeout": "40m",
            "type": "urltest",
            "url": "https://www.google.com/generate_204",
            "outbounds": null
        },
        {
            "tag": "direct",
            "type": "direct"
        },
        {
            "tag": "bypass",
            "type": "direct"
        },
        {
            "tag": "block",
            "type": "block"
        },
        {
            "tag": "dns-out",
            "type": "dns"
        }
    ],
    "route": {
        "auto_detect_interface": true,
        "rule_set": [
            {
                "download_detour": "proxy",
                "format": "binary",
                "tag": "geosite-ir",
                "type": "remote",
                "update_interval": "3d",
                "url": "https://cdn.statically.io/gh/chocolate4u/Iran-sing-box-rules@rule-set/geosite-ir.srs"
            },
            {
                "download_detour": "proxy",
                "format": "binary",
                "tag": "geoip-ir",
                "type": "remote",
                "update_interval": "3d",
                "url": "https://cdn.statically.io/gh/chocolate4u/Iran-sing-box-rules@rule-set/geoip-ir.srs"
            }
        ],
        "rules": [
            {
                "outbound": "dns-out",
                "port": [
                    53
                ]
            },
            {
                "inbound": [
                    "dns-in"
                ],
                "outbound": "dns-out"
            },
            {
                "ip_cidr": [
                    "224.0.0.0/3",
                    "ff00::/8"
                ],
                "outbound": "block",
                "source_ip_cidr": [
                    "224.0.0.0/3",
                    "ff00::/8"
                ]
            },
            {
                "ip_is_private": true,
                "outbound": "direct"
            },
            {
                "domain_suffix": ".ir",
                "outbound": "direct"
            },
            {
                "outbound": "direct",
                "rule_set": [
                    "geosite-ir",
                    "geoip-ir"
                ]
            }
        ]
    }
}
